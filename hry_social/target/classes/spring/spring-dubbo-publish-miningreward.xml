<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd  
        http://code.alibabatech.com/schema/dubbo  
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd  
        ">

    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~接口实现和发布~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- 果园挖矿奖励-具体的实现bean -->
    <bean id="remoteMiningRewardServiceImpl" class="hry.social.manage.remote.miningreward.RemoteMiningRewardServiceImpl"/>
    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service ref="remoteMiningRewardServiceImpl" interface="hry.social.manage.remote.api.miningreward.RemoteMiningRewardService"/>
</beans>  
