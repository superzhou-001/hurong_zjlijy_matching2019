<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="hry.social.sns.dao.SocialSnsVideoDao">

    <select id="getVideos" resultType="java.lang.String">
		SELECT
			path
		FROM
			social_sns_video
		WHERE
			postsId = #{postsId}
    </select>

</mapper>
